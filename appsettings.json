{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ValidationSettings": {
    "PasswordMinLength": 8,
    "PasswordMaxLength": 64,
    "PasswordRequireUppercase": true,
    "PasswordRequireLowercase": true,
    "PasswordRequireDigit": true,
    "PasswordRequireSpecial": true,
    "EmailMaxLength": 255,
    "FullNameMaxLength": 100
  }
}
